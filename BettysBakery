import numpy as np
cupcakes = np.array([2.00, 0.75, 2.00, 1.00, 0.50])
recipes = np.genfromtxt('recipes.csv', delimiter=',')
print recipes
eggs = recipes[:,2]
one_egg = eggs == 1
#print one_egg
cookies = recipes[2,:]
double_batch = cupcakes * 2
grocery_list = cookies + double_batch
print grocery_list
